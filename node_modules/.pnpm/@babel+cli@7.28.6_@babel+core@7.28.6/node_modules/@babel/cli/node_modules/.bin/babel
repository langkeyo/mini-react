#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/langkeyo/codes/temps/demo2/node_modules/.pnpm/@babel+cli@7.28.6_@babel+core@7.28.6/node_modules/@babel/cli/bin/node_modules:/home/langkeyo/codes/temps/demo2/node_modules/.pnpm/@babel+cli@7.28.6_@babel+core@7.28.6/node_modules/@babel/cli/node_modules:/home/langkeyo/codes/temps/demo2/node_modules/.pnpm/@babel+cli@7.28.6_@babel+core@7.28.6/node_modules/@babel/node_modules:/home/langkeyo/codes/temps/demo2/node_modules/.pnpm/@babel+cli@7.28.6_@babel+core@7.28.6/node_modules:/home/langkeyo/codes/temps/demo2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/langkeyo/codes/temps/demo2/node_modules/.pnpm/@babel+cli@7.28.6_@babel+core@7.28.6/node_modules/@babel/cli/bin/node_modules:/home/langkeyo/codes/temps/demo2/node_modules/.pnpm/@babel+cli@7.28.6_@babel+core@7.28.6/node_modules/@babel/cli/node_modules:/home/langkeyo/codes/temps/demo2/node_modules/.pnpm/@babel+cli@7.28.6_@babel+core@7.28.6/node_modules/@babel/node_modules:/home/langkeyo/codes/temps/demo2/node_modules/.pnpm/@babel+cli@7.28.6_@babel+core@7.28.6/node_modules:/home/langkeyo/codes/temps/demo2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/babel.js" "$@"
else
  exec node  "$basedir/../../bin/babel.js" "$@"
fi
